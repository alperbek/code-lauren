{
  "examples": [
    {
      "code":      ["write(1, 1)", "write(1, )"],
      "stack":     ["argument", "expression", "atom", "number"],
      "nextInput": ",",
      "message":   "No comma required"
    },

    {
      "code":      "1a",
      "stack":     ["number"],
      "nextInput": "[a-zA-Z0-9_\\-]+",
      "message":   "Can't have a name that starts with a number"
    },

    {
      "code":      "1()",
      "stack":     ["expression", "atom", "number"],
      "nextInput": "\\(\\)",
      "message":   "Can't run a number - it's not an action"
    },

    {
      "code":      "a: ",
      "stack":     ["assignment", "_"],
      "nextInput": "\n*$",
      "message":   "Name needs a value"
    },

    {
      "code":      "a ()",
      "stack":     ["_"],
      "nextInput": "\\(\\)",
      "message":   "Should be no spaces between\nthe name of the action and the ()"
    },

    {
      "code":      "a:",
      "stack":     ["assignment", "colon"],
      "nextInput": "\n*$",
      "message":   "Name needs a value"
    },

    {
      "code":      ": 1",
      "stack":     ["start"],
      "nextInput": ":",
      "message":   "Needs a name on the left and a value on the right"
    },

    {
      "code":      "a: 1\n: 1",
      "stack":     ["nl"],
      "nextInput": ":",
      "message":   "Needs a name on the left and a value on the right"
    },

    {
      "code":      "a: 1\n: 1",
      "stack":     ["nl"],
      "nextInput": ":",
      "message":   "Needs a name on the left and a value on the right"
    },

    {
      "code":      "a b",
      "stack":     ["_"],
      "nextInput": "[a-zA-Z0-9_\\-]+",
      "message":   "Expected this to be on the next line"
    }
  ]
}
